# Firecrawl API
FIRECRAWL_API_KEY=your_api_key_here

# Output Settings
OUTPUT_DIR=../../docs/3rd-party-docs  # Default output directory relative to utility
MAX_FILE_SIZE=10                      # Maximum file size in MB

# API Settings
